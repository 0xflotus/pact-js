{
  "name": "pact-dsl",
  "version": "0.0.1",
  "description": "Pact DSL for all things Javascript",
  "main": "./dist/pact-dsl.js",
  "scripts": {
    "pretest": "npm run lint && npm run jscpd",
    "lint": "standard",
    "jscpd": "jscpd -p src -r json -o jscpd.json",
    "dist": "npm run dist:web && npm run dist:node",
    "dist:web": "webpack --config ./config/webpack.web.config.js",
    "dist:node": "webpack --config ./config/webpack.node.config.js",
    "predist": "npm t",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- ./test"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "ia32"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tarciosaraiva/pact-js.git"
  },
  "keywords": [
    "pact",
    "pact-js",
    "javascript",
    "contract testing",
    "testing",
    "consumer driven testing"
  ],
  "author": "Beth Skurrie <beth@bethesque.com> (https://github.com/bethesque)",
  "contributors": [
    {
      "name": "Tarcio Saraiva",
      "email": "tarcio@gmail.com",
      "url": "http://twitter.com/tarciosaraiva"
    },
    {
      "name": "David Stanciu",
      "email": "davidstanciu29@gmail.com",
      "url": "https://github.com/dstanciu29"
    },
    {
      "name": "Michel Boudreau",
      "email": "michelboudreau@gmail.com",
      "url": "http://codinghitchhiker.com"
    },
    {
      "name": "Fu Ying",
      "email": "fu.ying.er@rea-group.com"
    },
    {
      "name": "Malinda Kapuruge",
      "email": "kaushalye@gmail.com"
    },
    {
      "name": "Ben Sayers",
      "email": "bpsayers@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tarciosaraiva/pact-js/issues"
  },
  "homepage": "http://docs.pact.io/documentation/javascript.html",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "test/**",
      "dist/**"
    ]
  },
  "dependencies": {
    "es6-promise": "3.2.1",
    "lodash.clone": "4.3.2",
    "lodash.isfunction": "3.0.8",
    "lodash.isnil": "4.0.0",
    "lodash.isundefined": "3.0.1",
    "lodash.omitby": "4.4.0"
  },
  "devDependencies": {
    "@pact-foundation/pact-node": "2.0.1",
    "babel-cli": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-register": "6.9.0",
    "bluebird": "3.4.0",
    "body-parser": "1.15.1",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "cors": "2.7.1",
    "express": "4.13.4",
    "imports-loader": "0.6.5",
    "isparta": "4.0.0",
    "istanbul": "0.4.3",
    "jscpd": "0.6.1",
    "json-loader": "0.5.4",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "pact-interceptor": "file:../pact-interceptor",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "standard": "7.1.0",
    "superagent-bluebird-promise": "3.0.0",
    "webpack": "1.13.1"
  }
}
