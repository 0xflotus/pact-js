{
  "name": "pact-mocha-interface",
  "version": "0.0.1",
  "description": "Mocha Interface for Pact",
  "main": "src/index.js",
  "scripts": {
    "pretest": "npm run lint && npm run jscpd",
    "lint": "standard",
    "jscpd": "jscpd -p src -r json -o jscpd.json",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- --require ./src/index.js --ui pact ./test"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "ia32"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tarciosaraiva/pact-js.git"
  },
  "keywords": [
    "pact",
    "pact-js",
    "javascript",
    "contract testing"
  ],
  "author": "Beth Skurrie <beth@bethesque.com> (https://github.com/bethesque)",
  "contributors": [
    {
      "name": "Tarcio Saraiva",
      "email": "tarcio@gmail.com",
